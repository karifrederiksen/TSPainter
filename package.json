{
    "name": "painter",
    "version": "0.1.0",
    "scripts": {
        "start": "npx webpack-serve --config webpack.dev.js --clipboard --log-time",
        "start-css": "npx tcm src --camelCase --watch",
        "build-dev": "npx webpack-cli --config webpack.dev.js",
        "build-prod": "npx webpack-cli --config webpack.prod.js",
        "typecheck": "npx tsc --project tsconfig.json --noEmit",
        "test": "npx jest --config jest.config.js",
        "generate": "npx tsm src --nameFormat \"camel\""
    },
    "dependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/runtime": "^7.3.4",
        "@types/jest": "^24.0.11",
        "@types/node": "^10.12.30",
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-node-env-inline": "^0.4.3",
        "clean-webpack-plugin": "^0.1.19",
        "color": "git+https://github.com/karifrederiksen/color.git",
        "css-loader": "^1.0.1",
        "fork-ts-checker-webpack-plugin": "1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "ivi": "^0.27.1",
        "ivi-html": "^0.27.0",
        "jest": "^24.3.1",
        "jsverify": "^0.8.4",
        "mini-css-extract-plugin": "^0.7.0",
        "node-sass": "^4.12.0",
        "ordered-collections": "git+https://github.com/karifrederiksen/ordered-collections.git",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.3",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "ts-node": "^7.0.1",
        "typed-scss-modules": "0.0.10",
        "typescript": "^3.5.1",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-serve": "^2.0.3"
    }
}
