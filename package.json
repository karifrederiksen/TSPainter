{
    "name": "painter",
    "version": "0.1.0",
    "license": "UNLICENSED",
    "scripts": {
        "start": "yarn webpack-serve --config webpack.dev.js --port 6789 --host localhost --compress --live-reload --wait-for-build --open --static dist/development",
        "start-css": "yarn tcm src --camelCase --watch",
        "build-dev": "yarn webpack-cli --config webpack.dev.js",
        "build-prod": "yarn webpack-cli --config webpack.prod.js",
        "typecheck": "yarn tsc --project tsconfig.json --noEmit",
        "test": "yarn ava",
        "generate": "yarn tsm src --nameFormat \"camel\""
    },
    "ava": {
        "require": [
            "esm",
            "ts-node/register"
        ],
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "files": [
            "**/*.test.ts"
        ],
        "sources": [
            "src"
        ]
    },
    "dependencies": {
        "@types/node": "10.14.10",
        "ava": "2.3.0",
        "clean-webpack-plugin": "0.1.19",
        "color": "git+https://github.com/karifrederiksen/color.git",
        "css-loader": "3.0.0",
        "esm": "3.2.25",
        "fork-ts-checker-webpack-plugin": "1.3.7",
        "html-webpack-plugin": "3.2.0",
        "ivi": "0.27.1",
        "ivi-html": "0.27.0",
        "ivi-svg": "0.27.0",
        "jsverify": "0.8.4",
        "material-design-icons": "3.0.1",
        "mini-css-extract-plugin": "0.7.0",
        "node-sass": "4.12.0",
        "ordered-collections": "git+https://github.com/karifrederiksen/ordered-collections.git",
        "sass-loader": "7.1.0",
        "terser-webpack-plugin": "1.3.0",
        "ts-loader": "6.0.4",
        "ts-node": "8.3.0",
        "typed-scss-modules": "0.0.10",
        "typescript": "3.5.2",
        "webpack": "4.35.0",
        "webpack-cli": "3.3.5",
        "webpack-serve": "3.1.0"
    }
}
